{
  "https_only": true,
  "root": "build/",
  "proxies": {
    "/api-beta/": {
      "origin": "${EMBER_API_DOCS_URL}"
    }
  },
  "error_page": "404.html",
  "headers": {
    "/images/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/javascripts/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/stylesheets/**": {
      "Cache-Control": "max-age=31536000"
    }
  },
  "redirects": {
    "/api-beta": {
      "url": "/api-beta/"
    }
  }
}

