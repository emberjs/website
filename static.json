{
  "https_only": true,
  "root": "build/",
  "proxies": {
    "/api/": {
      "origin": "${EMBER_API_DOCS_URL}"
    },
    "/api/ember/2.16/modules/@ember%2Fapplication": {
      "origin": "${EMBER_API_DOCS_URL}/ember/2.16/modules/@ember%2Fapplication"
    },
    "/api/ember/2.16/modules/@ember%2Farray": {
      "origin": "${EMBER_API_DOCS_URL}/ember/2.16/modules/@ember/array"
    },
    "/api/ember/2.16/modules/@ember/component": {
      "origin": "${EMBER_API_DOCS_URL}/ember/2.16/modules/@ember/component"
    },
    "/api/ember/2.16/modules/@ember/controller": {
      "origin": "${EMBER_API_DOCS_URL}/ember/2.16/modules/@ember%2Fcontroller"
    },
    "/statusboard": {
      "origin": "https://ember-learn.github.io/statusboard/"
    }
  },
  "error_page": "404.html",
  "headers": {
    "/images/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/javascripts/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/stylesheets/**": {
      "Cache-Control": "max-age=31536000"
    }
  },
  "redirects": {
    "/api": {
      "url": "/api/"
    },
    "/api-beta": {
      "url": "/api/"
    },
    "/api-beta/": {
      "url": "/api/"
    },
    "/internal": {
      "url": "/api/"
    },
    "/dashboard": {
      "url": "/statusboard"
    }
  }
}

