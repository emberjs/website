{
  "https_only": true,
  "root": "build/",
  "proxies": {
    "/api/": {
      "origin": "${EMBER_API_DOCS_URL}"
    },
	"/statusboard": {
	  "origin": "https://ember-learn.github.io/statusboard/"
	}
  },
  "error_page": "404.html",
  "headers": {
    "/images/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/javascripts/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/stylesheets/**": {
      "Cache-Control": "max-age=31536000"
    }
  },
  "redirects": {
    "/api": {
      "url": "/api/"
    },
    "/api-beta": {
      "url": "/api/"
    },
    "/api-beta/": {
      "url": "/api/"
    },
    "/internal": {
      "url": "/api/"
    }
  }
}

