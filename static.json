{
  "https_only": true,
  "root": "build/",
  "proxies": {
    "/api": {
      "origin": "${EMBER_API_DOCS_URL}"
    },
    "/statusboard": {
      "origin": "https://ember-learn.github.io/statusboard/"
    },
    "/deprecations": {
      "origin": "${DEPRECATION_APP_URL}"
    }
  },
  "error_page": "404.html",
  "headers": {
    "/images/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/javascripts/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/stylesheets/**": {
      "Cache-Control": "max-age=31536000"
    }
  },
  "redirects": {
    "/internal": {
      "url": "/api"
    },
    "/tomster/":  {
      "url": "/mascots/"
    },
    "/dashboard": {
      "url": "/statusboard"
    },
    "/guides": {
      "url": "https://guides.emberjs.com/"
    }
  }
}
